    "@babel/preset-flow": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-7-jest": "^21.3.3",
    "babel-eslint": "^9.0.0",
    "dotenv": "^6.1.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-flowtype": "^2.50.0",
